generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id    Int    @id @default(autoincrement())
  title String
  user  String @default("")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
}
